const { success, error, notFound } = require("../utils/responseHelpers");

exports.createCancellationReason = asyncHandler(async (req, res, next) => {
  const reason = await CancellationReason.create(req.body);
  return success(res, "cancellationReason.created", reason, 201);
});

exports.updateCancellationReason = asyncHandler(async (req, res, next) => {
  const reason = await CancellationReason.findByIdAndUpdate(
    req.params.id, req.body, { new: true }
  );
  if (!reason) return notFound(next, "cancellationReason");
  return success(res, "cancellationReason.updated", reason);
});

exports.deleteCancellationReason = asyncHandler(async (req, res, next) => {
  const reason = await CancellationReason.findByIdAndDelete(req.params.id);
  if (!reason) return notFound(next, "cancellationReason");
  return success(res, "cancellationReason.deleted");
});

exports.reorderCancellationReasons = asyncHandler(async (req, res, next) => {
  const { reasons } = req.body;
  for (const item of reasons) {
    await CancellationReason.findByIdAndUpdate(item.id, { order: item.order });
  }
  return success(res, "cancellationReason.reordered");
});
```

